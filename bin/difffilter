#!/usr/bin/env php
<?php
/**
 * Prints a filter to use with PHPUnit or CodeCeption based on what test cases cover the files changed in a diff.
 */

include $_composer_autoload_path ?? __DIR__ . '/../vendor/autoload.php';

$diffTest = new \BrianHenryIE\PhpDiffTest\DiffFilter();

$projectRootDir = getcwd();

$diffTest->run( $projectRootDir );